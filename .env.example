DATABASE_URL="postgresql://user:password@host:port/database"

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"

# Site URL (use https://app.boilerroom.ai in production, http://localhost:3000 in development)
NEXT_PUBLIC_SITE_URL="http://localhost:3000"

# People Data Labs API Key for prospecting search
# Get your API key from https://www.peopledatalabs.com/
PEOPLE_DATA_LABS_API_KEY="your_api_key_here"

# SendGrid API Key for email sending
# Get your API key from https://app.sendgrid.com/settings/api_keys
SENDGRID_API_KEY="your_sendgrid_api_key_here"
# SendGrid verified sender email (must be verified in SendGrid dashboard)
SENDGRID_FROM_EMAIL="noreply@yourdomain.com"

# Twilio API credentials for calling
# Get your credentials from https://console.twilio.com/
TWILIO_ACCOUNT_SID="your_twilio_account_sid"
TWILIO_AUTH_TOKEN="your_twilio_auth_token"
TWILIO_PHONE_NUMBER="+15551234567"

# Twilio API Key and Secret for browser calling (Voice SDK)
# Create an API Key at: https://console.twilio.com/us1/account/keys-credentials/api-keys
TWILIO_API_KEY="your_twilio_api_key"
TWILIO_API_SECRET="your_twilio_api_secret"

# Twilio TwiML Application SID for browser calling
# Create a TwiML App at: https://console.twilio.com/us1/develop/voice/manage/twiml-apps
# Set the Voice URL to: https://yourdomain.com/api/calls/twiml
TWILIO_TWIML_APP_SID="your_twiml_app_sid"

# NewsAPI.ai API Key for company insights
# Get your API key from https://newsapi.ai/
NEWSAPI_AI_KEY="your_newsapi_ai_api_key"

# AssemblyAI API Key for call transcription with speaker diarization
# Get your API key from https://www.assemblyai.com/
ASSEMBLYAI_API_KEY="your_assemblyai_api_key"
